{
  "name": "vic-oasis-astro",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "dev": "astro dev",
    "build": "astro build",
    "preview": "astro preview",
    "astro": "astro",
    "deploy": "npm run build && gh-pages -d dist",
    "lint": "eslint . --ext .js,.cjs,.mjs,.ts,.astro,.md,.mdx",
    "lint:fix": "eslint . --ext .js,.cjs,.mjs,.ts,.astro,.md,.mdx --fix",
    "format": "prettier --write .",
    "check": "astro check",
    "check:md": "node scripts/check-markdown.js",
    "check:md:all": "node scripts/validate-markdown.js",
    "mdx-to-md": "node scripts/mdx-to-md.cjs",
    "mdx-to-md:all": "node scripts/mdx-to-md.cjs --all",
    "pretest": "npm run check",
    "test": "npm run lint"
  },
  "dependencies": {
    "@astrojs/mdx": "^4.2.6",
    "@astrojs/tailwind": "^6.0.2",
    "astro": "^5.7.13",
    "tailwindcss": "^3.3.2"
  },
  "devDependencies": {
    "@eslint/js": "^9.27.0",
    "@tailwindcss/forms": "^0.5.10",
    "@typescript-eslint/eslint-plugin": "^8.32.1",
    "@typescript-eslint/parser": "^8.32.1",
    "autoprefixer": "^10.4.17",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-astro": "^1.3.1",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-mdx": "^3.4.2",
    "gh-pages": "^6.3.0",
    "glob": "^10.4.0",
    "gray-matter": "^4.0.3",
    "js-yaml": "^4.1.0",
    "markdownlint": "^0.38.0",
    "markdownlint-cli2": "^0.18.1",
    "postcss": "^8.4.35",
    "prettier": "^3.5.3",
    "prettier-plugin-astro": "^0.14.1",
    "rehype-raw": "^7.0.0",
    "remark-cli": "^12.0.1",
    "remark-directive": "^4.0.0",
    "remark-preset-lint-recommended": "^7.0.1",
    "typescript-eslint": "^8.32.1",
    "yaml-validator": "^5.0.0"
  }
}
